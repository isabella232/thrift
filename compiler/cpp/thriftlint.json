{
  "rules": [
    {
      "name": "enum_name",
      "message": "Enum names should start with OT and after should be PascalCase",
      "regex": "^OT[A-Za-z][A-Za-z0-9]+$"
    },
    {
      "name": "enum_constant_name",
      "message": "Enum constant names should be in snake case",
      "regex": "^[a-z][a-z0-9_]*[a-z]$",
      "enum_exceptions": [
        "OTPrivacyLevel",
        "OTDiagnosticConsentLevelAsInt",
        "OTPrivacyServiceStateAsInt",
        "OTPrivacySettingSourceLocationAsInt",
        "OTPrivacyIdentitySpace",
        "OTCIDType",
        "OTAccountType",
        "OTAccountCloud",
        "OTFolderType",
        "OTPrivacyAccountType",
        "OTPrivacySettingsAction",
        "OTPrivacySettingsResult",
        "OTPrivacySettingType",
        "OTPrivacyRoamingService",
        "OTPrivacySettingsFailureReason"
      ],
      "exceptions": [
        "underSubmit",
        "bottomOfScreen",
        "actionCard",
        "hideAd",
        "GROUPS",
        "OTHER",
        "GROUP",
        "OTHER",
        "fileDetails",
