        "zeroQuery",
        "includeDeletedToggle",
        "voiceAssistant",
        "spellerSuggestion",
        "closedWithSearch",
        "closedWithoutSearch",
        "closedWithTimeout",
        "closedWithError",
        "deniedPermissions",
        "latencyDiffFirstWord",
        "startedListening",
        "microphoneShown",
        "initializingStateShown",
        "peoplePhone",
        "peopleEmail",
        "peopleOfficeLocation",
        "MFU",
        "MRU",
        "microsoftGraph",
        "lokiAccessToken",
        "substrateToken",
        "cortanaToken",
        "featureAwareness",
        "newAccount",
        "changeServerSettings",
        "timeoutError",
        "authenticationError",
        "unknownError",
        "qrIntroShown",
        "qrIntroScanQrCode",
        "qrIntroSignInManually",
        "qrScanShown",
        "qrScanSignInManually",
        "qrScanSuccess",
        "qrScanFail",
        "lensSDK_camera",
        "enterBackgroundEarly",
